{
  "compilerOptions": {
    "target": "ES6",
    "lib": [
      "dom",
      "ESNext",
      "WebWorker"
    ],
    "module": "ESNext",
    "declaration": true,
    "emitDeclarationOnly": true,
    "outDir": "./dist",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "noImplicitAny": false,
    "noImplicitThis": false,
    "skipLibCheck": true
  }
}
